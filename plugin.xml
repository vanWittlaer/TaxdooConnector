<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../engine/Shopware/Components/Plugin/schema/plugin.xsd">
    <label>VanWittlaerTaxdooConnector</label>
    <label lang="de">VanWittlaerTaxdooConnector</label>
    <version>0.1.0</version>
    <copyright>(c) vanWittlaer GmbH</copyright>

    <license>AGPLv3</license>
    <link>https://vanwittlaer.de/</link>
    <author>vanWittlaer GmbH</author>

    <compatibility minVersion="5.5.1" />

    <changelog version="0.1.0">
        <changes>Simplify configuration for excluded channels.</changes>
        <changes lang="de">Vereinfachung der Konfiguration für ausgeschlossene Kanäle.</changes>
    </changelog>

    <changelog version="0.0.8">
        <changes>Removed pcntl functions.</changes>
        <changes lang="de">Nicht überall verfügbare pcntl-Funktionen aus Code entfernt.</changes>
    </changelog>

    <changelog version="0.0.6">
        <changes>Add support for plugin Ottscho Idealo Connector.</changes>
        <changes lang="de">Unterstützung für Ottscho Ideal Direktkaufanbindung.</changes>
    </changelog>

    <changelog version="0.0.5">
        <changes>Filter for blocked channels.</changes>
        <changes lang="de">Filter für geblockte Channels.</changes>
    </changelog>

    <changelog version="0.0.4">
        <changes>Initial release supporting PickwareERP support.</changes>
        <changes lang="de">Erster Release mit Unterstützung für PickwareERP.</changes>
    </changelog>

    <changelog version="0.0.3">
        <changes>Use default warehouse if none defined in plugin config.</changes>
        <changes lang="de">Verwende Taxdoo-Default-Warehouse wenn nichts anderes in Plugin-Configuration definiert.</changes>
    </changelog>

    <changelog version="0.0.2">
        <changes>Corrections in determining date intervals.</changes>
        <changes lang="de">Fehler bei Berechnung von Intervallen behoben.</changes>
    </changelog>

    <changelog version="0.0.1">
        <changes>Pre-release</changes>
        <changes lang="de">Pre-release</changes>
    </changelog>
</plugin>